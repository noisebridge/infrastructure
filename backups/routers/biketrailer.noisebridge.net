firewall {
    all-ping enable
    broadcast-ping disable
    ipv6-name WANv6_IN {
        default-action drop
        description "WAN inbound traffic forwarded to LAN"
        enable-default-log
        rule 10 {
            action accept
            description ICMPv6
            log disable
            protocol icmpv6
        }
        rule 20 {
            action accept
            description "Allow established"
            log disable
            protocol all
            state {
                established enable
                related enable
            }
        }
        rule 21 {
            action drop
            description "Drop invalid state"
            protocol all
            state {
                invalid enable
            }
        }
        rule 30 {
            action accept
            description "Colt TCP allowed"
            destination {
                address 2607:f598:b0cf:200::5
                port 22,80,443
            }
            protocol tcp
            state {
                new enable
            }
        }
        rule 32 {
            action accept
            description "Allow MOSH inbound"
            destination {
                port 60000-61000
            }
            protocol udp
        }
    }
    ipv6-name WANv6_LOCAL {
        default-action drop
        description "WAN inbound traffic to the router"
        enable-default-log
        rule 10 {
            action accept
            description ICMPv6
            log disable
            protocol icmpv6
        }
        rule 20 {
            action accept
            description "Allow established"
            log disable
            protocol all
            state {
                established enable
                related enable
            }
        }
        rule 21 {
            action drop
            description "Drop invalid state"
            state {
                invalid enable
            }
        }
        rule 30 {
            action accept
            description DHCPv6
            destination {
                port 546,547
            }
            log disable
            protocol udp
        }
        rule 31 {
            action accept
            description "Allow SSH"
            destination {
                port 22
            }
            protocol tcp
            state {
                new enable
            }
        }
    }
    ipv6-receive-redirects disable
    ipv6-src-route disable
    ip-src-route disable
    log-martians enable
    name WAN_LOCAL {
        default-action drop
        description "WAN to router"
        rule 10 {
            action accept
            description ICMP
            log disable
            protocol icmp
        }
        rule 20 {
            action accept
            description "Allow established"
            log disable
            protocol all
            state {
                established enable
                related enable
            }
        }
        rule 21 {
            action drop
            description "Drop invalid"
            log disable
            protocol all
            state {
                invalid enable
            }
        }
        rule 30 {
            action accept
            description "Allow SSH"
            destination {
                port 22
            }
            log disable
            protocol tcp
            state {
                new enable
            }
        }
        rule 40 {
            action accept
            description "Respond to ping"
            log disable
            protocol 1
        }
    }
    receive-redirects disable
    send-redirects enable
    source-validation disable
    syn-cookies enable
}
interfaces {
    ethernet eth0 {
        address 192.195.83.130/29
        address 2607:f598:b0c0:207::f002/64
        description Internet
        dhcpv6-pd {
            no-dns
            pd 1 {
                interface eth1 {
                    host-address ::1
                    no-dns
                    prefix-id :1
                    service dhcpv6-stateless
                }
                prefix-length 56
            }
            rapid-commit enable
        }
        duplex auto
        firewall {
            in {
                ipv6-name WANv6_IN
            }
            local {
                ipv6-name WANv6_LOCAL
                name WAN_LOCAL
            }
        }
        ip {
            enable-proxy-arp
        }
        ipv6 {
            address {
                autoconf
            }
            dup-addr-detect-transmits 1
        }
        speed auto
    }
    ethernet eth1 {
        address 10.21.0.1/16
        address 2607:F598:B0CF:0200::1/64
        description Local
        duplex auto
        speed auto
    }
    ethernet eth2 {
        address 192.168.2.1/24
        description "Local 2"
        duplex auto
        speed auto
    }
    ethernet eth3 {
        duplex auto
        speed auto
    }
    loopback lo {
    }
}
port-forward {
    auto-firewall enable
    hairpin-nat enable
    lan-interface eth1
    wan-interface eth0
}
protocols {
    static {
        route 0.0.0.0/0 {
            next-hop 192.195.83.129 {
